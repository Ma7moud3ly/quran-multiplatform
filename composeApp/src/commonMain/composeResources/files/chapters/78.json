{
    "id": 78,
    "name": "النبإ",
    "transliteration": "An-Naba",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "عَمَّ يَتَسَآءَلُونَ",
            "textNormalized": "عم يتساءلون",
            "transliteration": "AAamma yatasaaloona"
        },
        {
            "id": 2,
            "text": "عَنِ ٱلنَّبَإِ ٱلۡعَظِيمِ",
            "textNormalized": "عن النبا العظيم",
            "transliteration": "AAani alnnabai alAAatheemi"
        },
        {
            "id": 3,
            "text": "ٱلَّذِي هُمۡ فِيهِ مُخۡتَلِفُونَ",
            "textNormalized": "الذي هم فيه مختلفون",
            "transliteration": "Allathee hum feehi mukhtalifoona"
        },
        {
            "id": 4,
            "text": "كَلَّا سَيَعۡلَمُونَ",
            "textNormalized": "كلا سيعلمون",
            "transliteration": "Kalla sayaAAlamoona"
        },
        {
            "id": 5,
            "text": "ثُمَّ كَلَّا سَيَعۡلَمُونَ",
            "textNormalized": "ثم كلا سيعلمون",
            "transliteration": "Thumma kalla sayaAAlamoona"
        },
        {
            "id": 6,
            "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ مِهَٰدٗا",
            "textNormalized": "الم نجعل الارض مهدا",
            "transliteration": "Alam najAAali alarda mihadan"
        },
        {
            "id": 7,
            "text": "وَٱلۡجِبَالَ أَوۡتَادٗا",
            "textNormalized": "والجبال اوتادا",
            "transliteration": "Waaljibala awtadan"
        },
        {
            "id": 8,
            "text": "وَخَلَقۡنَٰكُمۡ أَزۡوَٰجٗا",
            "textNormalized": "وخلقنكم ازوجا",
            "transliteration": "Wakhalaqnakum azwajan"
        },
        {
            "id": 9,
            "text": "وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتٗا",
            "textNormalized": "وجعلنا نومكم سباتا",
            "transliteration": "WajaAAalna nawmakum subatan"
        },
        {
            "id": 10,
            "text": "وَجَعَلۡنَا ٱلَّيۡلَ لِبَاسٗا",
            "textNormalized": "وجعلنا اليل لباسا",
            "transliteration": "WajaAAalna allayla libasan"
        },
        {
            "id": 11,
            "text": "وَجَعَلۡنَا ٱلنَّهَارَ مَعَاشٗا",
            "textNormalized": "وجعلنا النهار معاشا",
            "transliteration": "WajaAAalna alnnahara maAAashan"
        },
        {
            "id": 12,
            "text": "وَبَنَيۡنَا فَوۡقَكُمۡ سَبۡعٗا شِدَادٗا",
            "textNormalized": "وبنينا فوقكم سبعا شدادا",
            "transliteration": "Wabanayna fawqakum sabAAan shidadan"
        },
        {
            "id": 13,
            "text": "وَجَعَلۡنَا سِرَاجٗا وَهَّاجٗا",
            "textNormalized": "وجعلنا سراجا وهاجا",
            "transliteration": "WajaAAalna sirajan wahhajan"
        },
        {
            "id": 14,
            "text": "وَأَنزَلۡنَا مِنَ ٱلۡمُعۡصِرَٰتِ مَآءٗ ثَجَّاجٗا",
            "textNormalized": "وانزلنا من المعصرت ماء ثجاجا",
            "transliteration": "Waanzalna mina almuAAsirati maan thajjajan"
        },
        {
            "id": 15,
            "text": "لِّنُخۡرِجَ بِهِۦ حَبّٗا وَنَبَاتٗا",
            "textNormalized": "لنخرج به حبا ونباتا",
            "transliteration": "Linukhrija bihi habban wanabatan"
        },
        {
            "id": 16,
            "text": "وَجَنَّـٰتٍ أَلۡفَافًا",
            "textNormalized": "وجنت الفافا",
            "transliteration": "Wajannatin alfafan"
        },
        {
            "id": 17,
            "text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ كَانَ مِيقَٰتٗا",
            "textNormalized": "ان يوم الفصل كان ميقتا",
            "transliteration": "Inna yawma alfasli kana meeqatan"
        },
        {
            "id": 18,
            "text": "يَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَتَأۡتُونَ أَفۡوَاجٗا",
            "textNormalized": "يوم ينفخ في الصور فتاتون افواجا",
            "transliteration": "Yawma yunfakhu fee alssoori fatatoona afwajan"
        },
        {
            "id": 19,
            "text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبٗا",
            "textNormalized": "وفتحت السماء فكانت ابوبا",
            "transliteration": "Wafutihati alssamao fakanat abwaban"
        },
        {
            "id": 20,
            "text": "وَسُيِّرَتِ ٱلۡجِبَالُ فَكَانَتۡ سَرَابًا",
            "textNormalized": "وسيرت الجبال فكانت سرابا",
            "transliteration": "Wasuyyirati aljibalu fakanat saraban"
        },
        {
            "id": 21,
            "text": "إِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادٗا",
            "textNormalized": "ان جهنم كانت مرصادا",
            "transliteration": "Inna jahannama kanat mirsadan"
        },
        {
            "id": 22,
            "text": "لِّلطَّـٰغِينَ مَـَٔابٗا",
            "textNormalized": "للطغين مابا",
            "transliteration": "Lilttagheena maaban"
        },
        {
            "id": 23,
            "text": "لَّـٰبِثِينَ فِيهَآ أَحۡقَابٗا",
            "textNormalized": "لبثين فيها احقابا",
            "transliteration": "Labitheena feeha ahqaban"
        },
        {
            "id": 24,
            "text": "لَّا يَذُوقُونَ فِيهَا بَرۡدٗا وَلَا شَرَابًا",
            "textNormalized": "لا يذوقون فيها بردا ولا شرابا",
            "transliteration": "La yathooqoona feeha bardan wala sharaban"
        },
        {
            "id": 25,
            "text": "إِلَّا حَمِيمٗا وَغَسَّاقٗا",
            "textNormalized": "الا حميما وغساقا",
            "transliteration": "Illa hameeman waghassaqan"
        },
        {
            "id": 26,
            "text": "جَزَآءٗ وِفَاقًا",
            "textNormalized": "جزاء وفاقا",
            "transliteration": "Jazaan wifaqan"
        },
        {
            "id": 27,
            "text": "إِنَّهُمۡ كَانُواْ لَا يَرۡجُونَ حِسَابٗا",
            "textNormalized": "انهم كانوا لا يرجون حسابا",
            "transliteration": "Innahum kanoo la yarjoona hisaban"
        },
        {
            "id": 28,
            "text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابٗا",
            "textNormalized": "وكذبوا بايتنا كذابا",
            "transliteration": "Wakaththaboo biayatina kiththaban"
        },
        {
            "id": 29,
            "text": "وَكُلَّ شَيۡءٍ أَحۡصَيۡنَٰهُ كِتَٰبٗا",
            "textNormalized": "وكل شيء احصينه كتبا",
            "transliteration": "Wakulla shayin ahsaynahu kitaban"
        },
        {
            "id": 30,
            "text": "فَذُوقُواْ فَلَن نَّزِيدَكُمۡ إِلَّا عَذَابًا",
            "textNormalized": "فذوقوا فلن نزيدكم الا عذابا",
            "transliteration": "Fathooqoo falan nazeedakum illa AAathaban"
        },
        {
            "id": 31,
            "text": "إِنَّ لِلۡمُتَّقِينَ مَفَازًا",
            "textNormalized": "ان للمتقين مفازا",
            "transliteration": "Inna lilmuttaqeena mafazan"
        },
        {
            "id": 32,
            "text": "حَدَآئِقَ وَأَعۡنَٰبٗا",
            "textNormalized": "حدايق واعنبا",
            "transliteration": "Hadaiqa waaAAnaban"
        },
        {
            "id": 33,
            "text": "وَكَوَاعِبَ أَتۡرَابٗا",
            "textNormalized": "وكواعب اترابا",
            "transliteration": "WakawaAAiba atraban"
        },
        {
            "id": 34,
            "text": "وَكَأۡسٗا دِهَاقٗا",
            "textNormalized": "وكاسا دهاقا",
            "transliteration": "Wakasan dihaqan"
        },
        {
            "id": 35,
            "text": "لَّا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا كِذَّـٰبٗا",
            "textNormalized": "لا يسمعون فيها لغوا ولا كذبا",
            "transliteration": "La yasmaAAoona feeha laghwan wala kiththaban"
        },
        {
            "id": 36,
            "text": "جَزَآءٗ مِّن رَّبِّكَ عَطَآءً حِسَابٗا",
            "textNormalized": "جزاء من ربك عطاء حسابا",
            "transliteration": "Jazaan min rabbika AAataan hisaban"
        },
        {
            "id": 37,
            "text": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا ٱلرَّحۡمَٰنِۖ لَا يَمۡلِكُونَ مِنۡهُ خِطَابٗا",
            "textNormalized": "رب السموت والارض وما بينهما الرحمن لا يملكون منه خطابا",
            "transliteration": "Rabbi alssamawati waalardi wama baynahuma alrrahmani la yamlikoona minhu khitaban"
        },
        {
            "id": 38,
            "text": "يَوۡمَ يَقُومُ ٱلرُّوحُ وَٱلۡمَلَـٰٓئِكَةُ صَفّٗاۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنۡ أَذِنَ لَهُ ٱلرَّحۡمَٰنُ وَقَالَ صَوَابٗا",
            "textNormalized": "يوم يقوم الروح والمليكه صفا لا يتكلمون الا من اذن له الرحمن وقال صوابا",
            "transliteration": "Yawma yaqoomu alrroohu waalmalaikatu saffan la yatakallamoona illa man athina lahu alrrahmanu waqala sawaban"
        },
        {
            "id": 39,
            "text": "ذَٰلِكَ ٱلۡيَوۡمُ ٱلۡحَقُّۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
            "textNormalized": "ذلك اليوم الحق فمن شاء اتخذ الي ربه مابا",
            "transliteration": "Thalika alyawmu alhaqqu faman shaa ittakhatha ila rabbihi maaban"
        },
        {
            "id": 40,
            "text": "إِنَّآ أَنذَرۡنَٰكُمۡ عَذَابٗا قَرِيبٗا يَوۡمَ يَنظُرُ ٱلۡمَرۡءُ مَا قَدَّمَتۡ يَدَاهُ وَيَقُولُ ٱلۡكَافِرُ يَٰلَيۡتَنِي كُنتُ تُرَٰبَۢا",
            "textNormalized": "انا انذرنكم عذابا قريبا يوم ينظر المرء ما قدمت يداه ويقول الكافر يليتني كنت تربا",
            "transliteration": "Inna antharnakum AAathaban qareeban yawma yanthuru almaro ma qaddamat yadahu wayaqoolu alkafiru ya laytanee kuntu turaban"
        }
    ]
}