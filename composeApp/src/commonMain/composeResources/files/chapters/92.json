{
    "id": 92,
    "name": "الليل",
    "transliteration": "Al-Layl",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
            "textNormalized": "واليل اذا يغشي",
            "transliteration": "Waallayli itha yaghsha"
        },
        {
            "id": 2,
            "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
            "textNormalized": "والنهار اذا تجلي",
            "transliteration": "Waalnnahari itha tajalla"
        },
        {
            "id": 3,
            "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
            "textNormalized": "وما خلق الذكر والانثي",
            "transliteration": "Wama khalaqa alththakara waalontha"
        },
        {
            "id": 4,
            "text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
            "textNormalized": "ان سعيكم لشتي",
            "transliteration": "Inna saAAyakum lashatta"
        },
        {
            "id": 5,
            "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
            "textNormalized": "فاما من اعطي واتقي",
            "transliteration": "Faamma man aAAta waittaqa"
        },
        {
            "id": 6,
            "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
            "textNormalized": "وصدق بالحسني",
            "transliteration": "Wasaddaqa bialhusna"
        },
        {
            "id": 7,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
            "textNormalized": "فسنيسره لليسري",
            "transliteration": "Fasanuyassiruhu lilyusra"
        },
        {
            "id": 8,
            "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
            "textNormalized": "واما من بخل واستغني",
            "transliteration": "Waamma man bakhila waistaghna"
        },
        {
            "id": 9,
            "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
            "textNormalized": "وكذب بالحسني",
            "transliteration": "Wakaththaba bialhusna"
        },
        {
            "id": 10,
            "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
            "textNormalized": "فسنيسره للعسري",
            "transliteration": "Fasanuyassiruhu lilAAusra"
        },
        {
            "id": 11,
            "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
            "textNormalized": "وما يغني عنه ماله اذا تردي",
            "transliteration": "Wama yughnee AAanhu maluhu itha taradda"
        },
        {
            "id": 12,
            "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
            "textNormalized": "ان علينا للهدي",
            "transliteration": "Inna AAalayna lalhuda"
        },
        {
            "id": 13,
            "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
            "textNormalized": "وان لنا للاخره والاولي",
            "transliteration": "Wainna lana lalakhirata waaloola"
        },
        {
            "id": 14,
            "text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
            "textNormalized": "فانذرتكم نارا تلظي",
            "transliteration": "Faanthartukum naran talaththa"
        },
        {
            "id": 15,
            "text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
            "textNormalized": "لا يصليها الا الاشقي",
            "transliteration": "La yaslaha illa alashqa"
        },
        {
            "id": 16,
            "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
            "textNormalized": "الذي كذب وتولي",
            "transliteration": "Allathee kaththaba watawalla"
        },
        {
            "id": 17,
            "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
            "textNormalized": "وسيجنبها الاتقي",
            "transliteration": "Wasayujannabuha alatqa"
        },
        {
            "id": 18,
            "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
            "textNormalized": "الذي يوتي ماله يتزكي",
            "transliteration": "Allathee yutee malahu yatazakka"
        },
        {
            "id": 19,
            "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
            "textNormalized": "وما لاحد عنده من نعمه تجزي",
            "transliteration": "Wama liahadin AAindahu min niAAmatin tujza"
        },
        {
            "id": 20,
            "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
            "textNormalized": "الا ابتغاء وجه ربه الاعلي",
            "transliteration": "Illa ibtighaa wajhi rabbihi alaAAla"
        },
        {
            "id": 21,
            "text": "وَلَسَوۡفَ يَرۡضَىٰ",
            "textNormalized": "ولسوف يرضي",
            "transliteration": "Walasawfa yarda"
        }
    ]
}