{
    "id": 102,
    "name": "التكاثر",
    "transliteration": "At-Takathur",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "أَلۡهَىٰكُمُ ٱلتَّكَاثُرُ",
            "textNormalized": "الهيكم التكاثر",
            "transliteration": "Alhakumu alttakathuru"
        },
        {
            "id": 2,
            "text": "حَتَّىٰ زُرۡتُمُ ٱلۡمَقَابِرَ",
            "textNormalized": "حتي زرتم المقابر",
            "transliteration": "Hatta zurtumu almaqabira"
        },
        {
            "id": 3,
            "text": "كَلَّا سَوۡفَ تَعۡلَمُونَ",
            "textNormalized": "كلا سوف تعلمون",
            "transliteration": "Kalla sawfa taAAlamoona"
        },
        {
            "id": 4,
            "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ",
            "textNormalized": "ثم كلا سوف تعلمون",
            "transliteration": "Thumma kalla sawfa taAAlamoona"
        },
        {
            "id": 5,
            "text": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡيَقِينِ",
            "textNormalized": "كلا لو تعلمون علم اليقين",
            "transliteration": "Kalla law taAAlamoona AAilma alyaqeeni"
        },
        {
            "id": 6,
            "text": "لَتَرَوُنَّ ٱلۡجَحِيمَ",
            "textNormalized": "لترون الجحيم",
            "transliteration": "Latarawunna aljaheema"
        },
        {
            "id": 7,
            "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ ٱلۡيَقِينِ",
            "textNormalized": "ثم لترونها عين اليقين",
            "transliteration": "Thumma latarawunnaha AAayna alyaqeeni"
        },
        {
            "id": 8,
            "text": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ ٱلنَّعِيمِ",
            "textNormalized": "ثم لتسلن يوميذ عن النعيم",
            "transliteration": "Thumma latusalunna yawmaithin AAani alnnaAAeemi"
        }
    ]
}