{
    "id": 95,
    "name": "التين",
    "transliteration": "At-Tin",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "وَٱلتِّينِ وَٱلزَّيۡتُونِ",
            "textNormalized": "والتين والزيتون",
            "transliteration": "Waaltteeni waalzzaytooni"
        },
        {
            "id": 2,
            "text": "وَطُورِ سِينِينَ",
            "textNormalized": "وطور سينين",
            "transliteration": "Watoori seeneena"
        },
        {
            "id": 3,
            "text": "وَهَٰذَا ٱلۡبَلَدِ ٱلۡأَمِينِ",
            "textNormalized": "وهذا البلد الامين",
            "transliteration": "Wahatha albaladi alameeni"
        },
        {
            "id": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِيٓ أَحۡسَنِ تَقۡوِيمٖ",
            "textNormalized": "لقد خلقنا الانسن في احسن تقويم",
            "transliteration": "Laqad khalaqna alinsana fee ahsani taqweemin"
        },
        {
            "id": 5,
            "text": "ثُمَّ رَدَدۡنَٰهُ أَسۡفَلَ سَٰفِلِينَ",
            "textNormalized": "ثم رددنه اسفل سفلين",
            "transliteration": "Thumma radadnahu asfala safileena"
        },
        {
            "id": 6,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّـٰلِحَٰتِ فَلَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونٖ",
            "textNormalized": "الا الذين ءامنوا وعملوا الصلحت فلهم اجر غير ممنون",
            "transliteration": "Illa allatheena amanoo waAAamiloo alssalihati falahum ajrun ghayru mamnoonin"
        },
        {
            "id": 7,
            "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِٱلدِّينِ",
            "textNormalized": "فما يكذبك بعد بالدين",
            "transliteration": "Fama yukaththibuka baAAdu bialddeeni"
        },
        {
            "id": 8,
            "text": "أَلَيۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَٰكِمِينَ",
            "textNormalized": "اليس الله باحكم الحكمين",
            "transliteration": "Alaysa Allahu biahkami alhakimeena"
        }
    ]
}