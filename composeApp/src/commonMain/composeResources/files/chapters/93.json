{
    "id": 93,
    "name": "الضحى",
    "transliteration": "Ad-Duhaa",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "وَٱلضُّحَىٰ",
            "textNormalized": "والضحي",
            "transliteration": "Waaldduha"
        },
        {
            "id": 2,
            "text": "وَٱلَّيۡلِ إِذَا سَجَىٰ",
            "textNormalized": "واليل اذا سجي",
            "transliteration": "Waallayli itha saja"
        },
        {
            "id": 3,
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
            "textNormalized": "ما ودعك ربك وما قلي",
            "transliteration": "Ma waddaAAaka rabbuka wama qala"
        },
        {
            "id": 4,
            "text": "وَلَلۡأٓخِرَةُ خَيۡرٞ لَّكَ مِنَ ٱلۡأُولَىٰ",
            "textNormalized": "وللاخره خير لك من الاولي",
            "transliteration": "Walalakhiratu khayrun laka mina aloola"
        },
        {
            "id": 5,
            "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰٓ",
            "textNormalized": "ولسوف يعطيك ربك فترضي",
            "transliteration": "Walasawfa yuAAteeka rabbuka fatarda"
        },
        {
            "id": 6,
            "text": "أَلَمۡ يَجِدۡكَ يَتِيمٗا فَـَٔاوَىٰ",
            "textNormalized": "الم يجدك يتيما فاوي",
            "transliteration": "Alam yajidka yateeman faawa"
        },
        {
            "id": 7,
            "text": "وَوَجَدَكَ ضَآلّٗا فَهَدَىٰ",
            "textNormalized": "ووجدك ضالا فهدي",
            "transliteration": "Wawajadaka dallan fahada"
        },
        {
            "id": 8,
            "text": "وَوَجَدَكَ عَآئِلٗا فَأَغۡنَىٰ",
            "textNormalized": "ووجدك عايلا فاغني",
            "transliteration": "Wawajadaka AAailan faaghna"
        },
        {
            "id": 9,
            "text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ",
            "textNormalized": "فاما اليتيم فلا تقهر",
            "transliteration": "Faamma alyateema fala taqhar"
        },
        {
            "id": 10,
            "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ",
            "textNormalized": "واما السايل فلا تنهر",
            "transliteration": "Waamma alssaila fala tanhar"
        },
        {
            "id": 11,
            "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ",
            "textNormalized": "واما بنعمه ربك فحدث",
            "transliteration": "Waamma biniAAmati rabbika fahaddith"
        }
    ]
}