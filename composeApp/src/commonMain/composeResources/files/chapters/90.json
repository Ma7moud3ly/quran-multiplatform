{
    "id": 90,
    "name": "البلد",
    "transliteration": "Al-Balad",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ",
            "textNormalized": "لا اقسم بهذا البلد",
            "transliteration": "La oqsimu bihatha albaladi"
        },
        {
            "id": 2,
            "text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ",
            "textNormalized": "وانت حل بهذا البلد",
            "transliteration": "Waanta hillun bihatha albaladi"
        },
        {
            "id": 3,
            "text": "وَوَالِدٖ وَمَا وَلَدَ",
            "textNormalized": "ووالد وما ولد",
            "transliteration": "Wawalidin wama walada"
        },
        {
            "id": 4,
            "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ",
            "textNormalized": "لقد خلقنا الانسن في كبد",
            "transliteration": "Laqad khalaqna alinsana fee kabadin"
        },
        {
            "id": 5,
            "text": "أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ",
            "textNormalized": "ايحسب ان لن يقدر عليه احد",
            "transliteration": "Ayahsabu an lan yaqdira AAalayhi ahadun"
        },
        {
            "id": 6,
            "text": "يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا",
            "textNormalized": "يقول اهلكت مالا لبدا",
            "transliteration": "Yaqoolu ahlaktu malan lubadan"
        },
        {
            "id": 7,
            "text": "أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ",
            "textNormalized": "ايحسب ان لم يره احد",
            "transliteration": "Ayahsabu an lam yarahu ahadun"
        },
        {
            "id": 8,
            "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ",
            "textNormalized": "الم نجعل له عينين",
            "transliteration": "Alam najAAal lahu AAaynayni"
        },
        {
            "id": 9,
            "text": "وَلِسَانٗا وَشَفَتَيۡنِ",
            "textNormalized": "ولسانا وشفتين",
            "transliteration": "Walisanan washafatayni"
        },
        {
            "id": 10,
            "text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ",
            "textNormalized": "وهدينه النجدين",
            "transliteration": "Wahadaynahu alnnajdayni"
        },
        {
            "id": 11,
            "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ",
            "textNormalized": "فلا اقتحم العقبه",
            "transliteration": "Fala iqtahama alAAaqabata"
        },
        {
            "id": 12,
            "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ",
            "textNormalized": "وما ادريك ما العقبه",
            "transliteration": "Wama adraka ma alAAaqabatu"
        },
        {
            "id": 13,
            "text": "فَكُّ رَقَبَةٍ",
            "textNormalized": "فك رقبه",
            "transliteration": "Fakku raqabatin"
        },
        {
            "id": 14,
            "text": "أَوۡ إِطۡعَٰمٞ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ",
            "textNormalized": "او اطعم في يوم ذي مسغبه",
            "transliteration": "Aw itAAamun fee yawmin thee masghabatin"
        },
        {
            "id": 15,
            "text": "يَتِيمٗا ذَا مَقۡرَبَةٍ",
            "textNormalized": "يتيما ذا مقربه",
            "transliteration": "Yateeman tha maqrabatin"
        },
        {
            "id": 16,
            "text": "أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ",
            "textNormalized": "او مسكينا ذا متربه",
            "transliteration": "Aw miskeenan tha matrabatin"
        },
        {
            "id": 17,
            "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ",
            "textNormalized": "ثم كان من الذين ءامنوا وتواصوا بالصبر وتواصوا بالمرحمه",
            "transliteration": "Thumma kana mina allatheena amanoo watawasaw bialssabri watawasaw bialmarhamati"
        },
        {
            "id": 18,
            "text": "أُوْلَـٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
            "textNormalized": "اوليك اصحب الميمنه",
            "transliteration": "Olaika ashabu almaymanati"
        },
        {
            "id": 19,
            "text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
            "textNormalized": "والذين كفروا بايتنا هم اصحب المشمه",
            "transliteration": "Waallatheena kafaroo biayatina hum ashabu almashamati"
        },
        {
            "id": 20,
            "text": "عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ",
            "textNormalized": "عليهم نار موصده",
            "transliteration": "AAalayhim narun musadatun"
        }
    ]
}