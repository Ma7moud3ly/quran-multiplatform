{
    "id": 79,
    "name": "النازعات",
    "transliteration": "An-Nazi'at",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "وَٱلنَّـٰزِعَٰتِ غَرۡقٗا",
            "textNormalized": "والنزعت غرقا",
            "transliteration": "WaalnnaziAAati gharqan"
        },
        {
            "id": 2,
            "text": "وَٱلنَّـٰشِطَٰتِ نَشۡطٗا",
            "textNormalized": "والنشطت نشطا",
            "transliteration": "Waalnnashitati nashtan"
        },
        {
            "id": 3,
            "text": "وَٱلسَّـٰبِحَٰتِ سَبۡحٗا",
            "textNormalized": "والسبحت سبحا",
            "transliteration": "Waalssabihati sabhan"
        },
        {
            "id": 4,
            "text": "فَٱلسَّـٰبِقَٰتِ سَبۡقٗا",
            "textNormalized": "فالسبقت سبقا",
            "transliteration": "Faalssabiqati sabqan"
        },
        {
            "id": 5,
            "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرٗا",
            "textNormalized": "فالمدبرت امرا",
            "transliteration": "Faalmudabbirati amran"
        },
        {
            "id": 6,
            "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ",
            "textNormalized": "يوم ترجف الراجفه",
            "transliteration": "Yawma tarjufu alrrajifatu"
        },
        {
            "id": 7,
            "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ",
            "textNormalized": "تتبعها الرادفه",
            "transliteration": "TatbaAAuha alrradifatu"
        },
        {
            "id": 8,
            "text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ",
            "textNormalized": "قلوب يوميذ واجفه",
            "transliteration": "Quloobun yawmaithin wajifatun"
        },
        {
            "id": 9,
            "text": "أَبۡصَٰرُهَا خَٰشِعَةٞ",
            "textNormalized": "ابصرها خشعه",
            "transliteration": "Absaruha khashiAAatun"
        },
        {
            "id": 10,
            "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ",
            "textNormalized": "يقولون اءنا لمردودون في الحافره",
            "transliteration": "Yaqooloona ainna lamardoodoona fee alhafirati"
        },
        {
            "id": 11,
            "text": "أَءِذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ",
            "textNormalized": "اءذا كنا عظما نخره",
            "transliteration": "Aitha kunna AAithaman nakhiratan"
        },
        {
            "id": 12,
            "text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ",
            "textNormalized": "قالوا تلك اذا كره خاسره",
            "transliteration": "Qaloo tilka ithan karratun khasiratun"
        },
        {
            "id": 13,
            "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ",
            "textNormalized": "فانما هي زجره وحده",
            "transliteration": "Fainnama hiya zajratun wahidatun"
        },
        {
            "id": 14,
            "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
            "textNormalized": "فاذا هم بالساهره",
            "transliteration": "Faitha hum bialssahirati"
        },
        {
            "id": 15,
            "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
            "textNormalized": "هل اتيك حديث موسي",
            "transliteration": "Hal ataka hadeethu moosa"
        },
        {
            "id": 16,
            "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى",
            "textNormalized": "اذ ناديه ربه بالواد المقدس طوي",
            "transliteration": "Ith nadahu rabbuhu bialwadi almuqaddasi tuwan"
        },
        {
            "id": 17,
            "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ",
            "textNormalized": "اذهب الي فرعون انه طغي",
            "transliteration": "Ithhab ila firAAawna innahu tagha"
        },
        {
            "id": 18,
            "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
            "textNormalized": "فقل هل لك الي ان تزكي",
            "transliteration": "Faqul hal laka ila an tazakka"
        },
        {
            "id": 19,
            "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ",
            "textNormalized": "واهديك الي ربك فتخشي",
            "transliteration": "Waahdiyaka ila rabbika fatakhsha"
        },
        {
            "id": 20,
            "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ",
            "textNormalized": "فاريه الايه الكبري",
            "transliteration": "Faarahu alayata alkubra"
        },
        {
            "id": 21,
            "text": "فَكَذَّبَ وَعَصَىٰ",
            "textNormalized": "فكذب وعصي",
            "transliteration": "Fakaththaba waAAasa"
        },
        {
            "id": 22,
            "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ",
            "textNormalized": "ثم ادبر يسعي",
            "transliteration": "Thumma adbara yasAAa"
        },
        {
            "id": 23,
            "text": "فَحَشَرَ فَنَادَىٰ",
            "textNormalized": "فحشر فنادي",
            "transliteration": "Fahashara fanada"
        },
        {
            "id": 24,
            "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ",
            "textNormalized": "فقال انا ربكم الاعلي",
            "transliteration": "Faqala ana rabbukumu alaAAla"
        },
        {
            "id": 25,
            "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ",
            "textNormalized": "فاخذه الله نكال الاخره والاولي",
            "transliteration": "Faakhathahu Allahu nakala alakhirati waaloola"
        },
        {
            "id": 26,
            "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشَىٰٓ",
            "textNormalized": "ان في ذلك لعبره لمن يخشي",
            "transliteration": "Inna fee thalika laAAibratan liman yakhsha"
        },
        {
            "id": 27,
            "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا",
            "textNormalized": "ءانتم اشد خلقا ام السماء بنيها",
            "transliteration": "Aantum ashaddu khalqan ami alssamao banaha"
        },
        {
            "id": 28,
            "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا",
            "textNormalized": "رفع سمكها فسويها",
            "transliteration": "RafaAAa samkaha fasawwaha"
        },
        {
            "id": 29,
            "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا",
            "textNormalized": "واغطش ليلها واخرج ضحيها",
            "transliteration": "Waaghtasha laylaha waakhraja duhaha"
        },
        {
            "id": 30,
            "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ",
            "textNormalized": "والارض بعد ذلك دحيها",
            "transliteration": "Waalarda baAAda thalika dahaha"
        },
        {
            "id": 31,
            "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا",
            "textNormalized": "اخرج منها ماءها ومرعيها",
            "transliteration": "Akhraja minha maaha wamarAAaha"
        },
        {
            "id": 32,
            "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا",
            "textNormalized": "والجبال ارسيها",
            "transliteration": "Waaljibala arsaha"
        },
        {
            "id": 33,
            "text": "مَتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
            "textNormalized": "متعا لكم ولانعمكم",
            "transliteration": "MataAAan lakum walianAAamikum"
        },
        {
            "id": 34,
            "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ",
            "textNormalized": "فاذا جاءت الطامه الكبري",
            "transliteration": "Faitha jaati alttammatu alkubra"
        },
        {
            "id": 35,
            "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ",
            "textNormalized": "يوم يتذكر الانسن ما سعي",
            "transliteration": "Yawma yatathakkaru alinsanu ma saAAa"
        },
        {
            "id": 36,
            "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ",
            "textNormalized": "وبرزت الجحيم لمن يري",
            "transliteration": "Waburrizati aljaheemu liman yara"
        },
        {
            "id": 37,
            "text": "فَأَمَّا مَن طَغَىٰ",
            "textNormalized": "فاما من طغي",
            "transliteration": "Faamma man tagha"
        },
        {
            "id": 38,
            "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
            "textNormalized": "وءاثر الحيوه الدنيا",
            "transliteration": "Waathara alhayata alddunya"
        },
        {
            "id": 39,
            "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ",
            "textNormalized": "فان الجحيم هي الماوي",
            "transliteration": "Fainna aljaheema hiya almawa"
        },
        {
            "id": 40,
            "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ",
            "textNormalized": "واما من خاف مقام ربه ونهي النفس عن الهوي",
            "transliteration": "Waama man khafa maqama rabbihi wanaha alnnafsa AAani alhawa"
        },
        {
            "id": 41,
            "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ",
            "textNormalized": "فان الجنه هي الماوي",
            "transliteration": "Fainna aljannata hiya almawa"
        },
        {
            "id": 42,
            "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا",
            "textNormalized": "يسلونك عن الساعه ايان مرسيها",
            "transliteration": "Yasaloonaka AAani alssaAAati ayyana mursaha"
        },
        {
            "id": 43,
            "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَآ",
            "textNormalized": "فيم انت من ذكريها",
            "transliteration": "Feema anta min thikraha"
        },
        {
            "id": 44,
            "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
            "textNormalized": "الي ربك منتهيها",
            "transliteration": "Ila rabbika muntahaha"
        },
        {
            "id": 45,
            "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا",
            "textNormalized": "انما انت منذر من يخشيها",
            "transliteration": "Innama anta munthiru man yakhshaha"
        },
        {
            "id": 46,
            "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوٓاْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا",
            "textNormalized": "كانهم يوم يرونها لم يلبثوا الا عشيه او ضحيها",
            "transliteration": "Kaannahum yawma yarawnaha lam yalbathoo illa AAashiyyatan aw duhaha"
        }
    ]
}