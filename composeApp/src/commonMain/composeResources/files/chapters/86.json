{
    "id": 86,
    "name": "الطارق",
    "transliteration": "At-Tariq",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ",
            "textNormalized": "والسماء والطارق",
            "transliteration": "Waalssamai waalttariqi"
        },
        {
            "id": 2,
            "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ",
            "textNormalized": "وما ادريك ما الطارق",
            "transliteration": "Wama adraka ma alttariqu"
        },
        {
            "id": 3,
            "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ",
            "textNormalized": "النجم الثاقب",
            "transliteration": "Alnnajmu alththaqibu"
        },
        {
            "id": 4,
            "text": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ",
            "textNormalized": "ان كل نفس لما عليها حافظ",
            "transliteration": "In kullu nafsin lamma AAalayha hafithun"
        },
        {
            "id": 5,
            "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ",
            "textNormalized": "فلينظر الانسن مم خلق",
            "transliteration": "Falyanthuri alinsanu mimma khuliqa"
        },
        {
            "id": 6,
            "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ",
            "textNormalized": "خلق من ماء دافق",
            "transliteration": "Khuliqa min main dafiqin"
        },
        {
            "id": 7,
            "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ",
            "textNormalized": "يخرج من بين الصلب والترايب",
            "transliteration": "Yakhruju min bayni alssulbi waalttaraibi"
        },
        {
            "id": 8,
            "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ",
            "textNormalized": "انه علي رجعه لقادر",
            "transliteration": "Innahu AAala rajAAihi laqadirun"
        },
        {
            "id": 9,
            "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ",
            "textNormalized": "يوم تبلي السراير",
            "transliteration": "Yawma tubla alssarairu"
        },
        {
            "id": 10,
            "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ",
            "textNormalized": "فما له من قوه ولا ناصر",
            "transliteration": "Fama lahu min quwwatin wala nasirin"
        },
        {
            "id": 11,
            "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ",
            "textNormalized": "والسماء ذات الرجع",
            "transliteration": "Waalssamai thati alrrajAAi"
        },
        {
            "id": 12,
            "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ",
            "textNormalized": "والارض ذات الصدع",
            "transliteration": "Waalardi thati alssadAAi"
        },
        {
            "id": 13,
            "text": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ",
            "textNormalized": "انه لقول فصل",
            "transliteration": "Innahu laqawlun faslun"
        },
        {
            "id": 14,
            "text": "وَمَا هُوَ بِٱلۡهَزۡلِ",
            "textNormalized": "وما هو بالهزل",
            "transliteration": "Wama huwa bialhazli"
        },
        {
            "id": 15,
            "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا",
            "textNormalized": "انهم يكيدون كيدا",
            "transliteration": "Innahum yakeedoona kaydan"
        },
        {
            "id": 16,
            "text": "وَأَكِيدُ كَيۡدٗا",
            "textNormalized": "واكيد كيدا",
            "transliteration": "Waakeedu kaydan"
        },
        {
            "id": 17,
            "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا",
            "textNormalized": "فمهل الكفرين امهلهم رويدا",
            "transliteration": "Famahhili alkafireena amhilhum ruwaydan"
        }
    ]
}