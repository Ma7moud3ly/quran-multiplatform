{
    "id": 37,
    "name": "الصافات",
    "transliteration": "As-Saffat",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "وَٱلصَّـٰٓفَّـٰتِ صَفّٗا",
            "textNormalized": "والصفت صفا",
            "transliteration": "Waalssaffati saffan"
        },
        {
            "id": 2,
            "text": "فَٱلزَّـٰجِرَٰتِ زَجۡرٗا",
            "textNormalized": "فالزجرت زجرا",
            "transliteration": "Faalzzajirati zajran"
        },
        {
            "id": 3,
            "text": "فَٱلتَّـٰلِيَٰتِ ذِكۡرًا",
            "textNormalized": "فالتليت ذكرا",
            "transliteration": "Faalttaliyati thikran"
        },
        {
            "id": 4,
            "text": "إِنَّ إِلَٰهَكُمۡ لَوَٰحِدٞ",
            "textNormalized": "ان الهكم لوحد",
            "transliteration": "Inna ilahakum lawahidun"
        },
        {
            "id": 5,
            "text": "رَّبُّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا وَرَبُّ ٱلۡمَشَٰرِقِ",
            "textNormalized": "رب السموت والارض وما بينهما ورب المشرق",
            "transliteration": "Rabbu alssamawati waalardi wama baynahuma warabbu almashariqi"
        },
        {
            "id": 6,
            "text": "إِنَّا زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنۡيَا بِزِينَةٍ ٱلۡكَوَاكِبِ",
            "textNormalized": "انا زينا السماء الدنيا بزينه الكواكب",
            "transliteration": "Inna zayyanna alssamaa alddunya bizeenatin alkawakibi"
        },
        {
            "id": 7,
            "text": "وَحِفۡظٗا مِّن كُلِّ شَيۡطَٰنٖ مَّارِدٖ",
            "textNormalized": "وحفظا من كل شيطن مارد",
            "transliteration": "Wahifthan min kulli shaytanin maridin"
        },
        {
            "id": 8,
            "text": "لَّا يَسَّمَّعُونَ إِلَى ٱلۡمَلَإِ ٱلۡأَعۡلَىٰ وَيُقۡذَفُونَ مِن كُلِّ جَانِبٖ",
            "textNormalized": "لا يسمعون الي الملا الاعلي ويقذفون من كل جانب",
            "transliteration": "La yassammaAAoona ila almalai alaAAla wayuqthafoona min kulli janibin"
        },
        {
            "id": 9,
            "text": "دُحُورٗاۖ وَلَهُمۡ عَذَابٞ وَاصِبٌ",
            "textNormalized": "دحورا ولهم عذاب واصب",
            "transliteration": "Duhooran walahum AAathabun wasibun"
        },
        {
            "id": 10,
            "text": "إِلَّا مَنۡ خَطِفَ ٱلۡخَطۡفَةَ فَأَتۡبَعَهُۥ شِهَابٞ ثَاقِبٞ",
            "textNormalized": "الا من خطف الخطفه فاتبعه شهاب ثاقب",
            "transliteration": "Illa man khatifa alkhatfata faatbaAAahu shihabun thaqibun"
        },
        {
            "id": 11,
            "text": "فَٱسۡتَفۡتِهِمۡ أَهُمۡ أَشَدُّ خَلۡقًا أَم مَّنۡ خَلَقۡنَآۚ إِنَّا خَلَقۡنَٰهُم مِّن طِينٖ لَّازِبِۭ",
            "textNormalized": "فاستفتهم اهم اشد خلقا ام من خلقنا انا خلقنهم من طين لازب",
            "transliteration": "Faistaftihim ahum ashaddu khalqan am man khalaqna inna khalaqnahum min teenin lazibin"
        },
        {
            "id": 12,
            "text": "بَلۡ عَجِبۡتَ وَيَسۡخَرُونَ",
            "textNormalized": "بل عجبت ويسخرون",
            "transliteration": "Bal AAajibta wayaskharoona"
        },
        {
            "id": 13,
            "text": "وَإِذَا ذُكِّرُواْ لَا يَذۡكُرُونَ",
            "textNormalized": "واذا ذكروا لا يذكرون",
            "transliteration": "Waitha thukkiroo la yathkuroona"
        },
        {
            "id": 14,
            "text": "وَإِذَا رَأَوۡاْ ءَايَةٗ يَسۡتَسۡخِرُونَ",
            "textNormalized": "واذا راوا ءايه يستسخرون",
            "transliteration": "Waitha raaw ayatan yastaskhiroona"
        },
        {
            "id": 15,
            "text": "وَقَالُوٓاْ إِنۡ هَٰذَآ إِلَّا سِحۡرٞ مُّبِينٌ",
            "textNormalized": "وقالوا ان هذا الا سحر مبين",
            "transliteration": "Waqaloo in hatha illa sihrun mubeenun"
        },
        {
            "id": 16,
            "text": "أَءِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ",
            "textNormalized": "اءذا متنا وكنا ترابا وعظما اءنا لمبعوثون",
            "transliteration": "Aitha mitna wakunna turaban waAAithaman ainna lamabAAoothoona"
        },
        {
            "id": 17,
            "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ",
            "textNormalized": "اوءاباونا الاولون",
            "transliteration": "Awaabaona alawwaloona"
        },
        {
            "id": 18,
            "text": "قُلۡ نَعَمۡ وَأَنتُمۡ دَٰخِرُونَ",
            "textNormalized": "قل نعم وانتم دخرون",
            "transliteration": "Qul naAAam waantum dakhiroona"
        },
        {
            "id": 19,
            "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ فَإِذَا هُمۡ يَنظُرُونَ",
            "textNormalized": "فانما هي زجره وحده فاذا هم ينظرون",
            "transliteration": "Fainnama hiya zajratun wahidatun faitha hum yanthuroona"
        },
        {
            "id": 20,
            "text": "وَقَالُواْ يَٰوَيۡلَنَا هَٰذَا يَوۡمُ ٱلدِّينِ",
            "textNormalized": "وقالوا يويلنا هذا يوم الدين",
            "transliteration": "Waqaloo ya waylana hatha yawmu alddeeni"
        },
        {
            "id": 21,
            "text": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِ ٱلَّذِي كُنتُم بِهِۦ تُكَذِّبُونَ",
            "textNormalized": "هذا يوم الفصل الذي كنتم به تكذبون",
            "transliteration": "Hatha yawmu alfasli allathee kuntum bihi tukaththiboona"
        },
        {
            "id": 22,
            "text": "۞ٱحۡشُرُواْ ٱلَّذِينَ ظَلَمُواْ وَأَزۡوَٰجَهُمۡ وَمَا كَانُواْ يَعۡبُدُونَ",
            "textNormalized": "احشروا الذين ظلموا وازوجهم وما كانوا يعبدون",
            "transliteration": "Ohshuroo allatheena thalamoo waazwajahum wama kanoo yaAAbudoona"
        },
        {
            "id": 23,
            "text": "مِن دُونِ ٱللَّهِ فَٱهۡدُوهُمۡ إِلَىٰ صِرَٰطِ ٱلۡجَحِيمِ",
            "textNormalized": "من دون الله فاهدوهم الي صرط الجحيم",
            "transliteration": "Min dooni Allahi faihdoohum ila sirati aljaheemi"
        },
        {
            "id": 24,
            "text": "وَقِفُوهُمۡۖ إِنَّهُم مَّسۡـُٔولُونَ",
            "textNormalized": "وقفوهم انهم مسولون",
            "transliteration": "Waqifoohum innahum masooloona"
        },
        {
            "id": 25,
            "text": "مَا لَكُمۡ لَا تَنَاصَرُونَ",
            "textNormalized": "ما لكم لا تناصرون",
            "transliteration": "Ma lakum la tanasaroona"
        },
        {
            "id": 26,
            "text": "بَلۡ هُمُ ٱلۡيَوۡمَ مُسۡتَسۡلِمُونَ",
            "textNormalized": "بل هم اليوم مستسلمون",
            "transliteration": "Bal humu alyawma mustaslimoona"
        },
        {
            "id": 27,
            "text": "وَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَسَآءَلُونَ",
            "textNormalized": "واقبل بعضهم علي بعض يتساءلون",
            "transliteration": "Waaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "id": 28,
            "text": "قَالُوٓاْ إِنَّكُمۡ كُنتُمۡ تَأۡتُونَنَا عَنِ ٱلۡيَمِينِ",
            "textNormalized": "قالوا انكم كنتم تاتوننا عن اليمين",
            "transliteration": "Qaloo innakum kuntum tatoonana AAani alyameeni"
        },
        {
            "id": 29,
            "text": "قَالُواْ بَل لَّمۡ تَكُونُواْ مُؤۡمِنِينَ",
            "textNormalized": "قالوا بل لم تكونوا مومنين",
            "transliteration": "Qaloo bal lam takoonoo mumineena"
        },
        {
            "id": 30,
            "text": "وَمَا كَانَ لَنَا عَلَيۡكُم مِّن سُلۡطَٰنِۭۖ بَلۡ كُنتُمۡ قَوۡمٗا طَٰغِينَ",
            "textNormalized": "وما كان لنا عليكم من سلطن بل كنتم قوما طغين",
            "transliteration": "Wama kana lana AAalaykum min sultanin bal kuntum qawman tagheena"
        },
        {
            "id": 31,
            "text": "فَحَقَّ عَلَيۡنَا قَوۡلُ رَبِّنَآۖ إِنَّا لَذَآئِقُونَ",
            "textNormalized": "فحق علينا قول ربنا انا لذايقون",
            "transliteration": "Fahaqqa AAalayna qawlu rabbina inna lathaiqoona"
        },
        {
            "id": 32,
            "text": "فَأَغۡوَيۡنَٰكُمۡ إِنَّا كُنَّا غَٰوِينَ",
            "textNormalized": "فاغوينكم انا كنا غوين",
            "transliteration": "Faaghwaynakum inna kunna ghaweena"
        },
        {
            "id": 33,
            "text": "فَإِنَّهُمۡ يَوۡمَئِذٖ فِي ٱلۡعَذَابِ مُشۡتَرِكُونَ",
            "textNormalized": "فانهم يوميذ في العذاب مشتركون",
            "transliteration": "Fainnahum yawmaithin fee alAAathabi mushtarikoona"
        },
        {
            "id": 34,
            "text": "إِنَّا كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ",
            "textNormalized": "انا كذلك نفعل بالمجرمين",
            "transliteration": "Inna kathalika nafAAalu bialmujrimeena"
        },
        {
            "id": 35,
            "text": "إِنَّهُمۡ كَانُوٓاْ إِذَا قِيلَ لَهُمۡ لَآ إِلَٰهَ إِلَّا ٱللَّهُ يَسۡتَكۡبِرُونَ",
            "textNormalized": "انهم كانوا اذا قيل لهم لا اله الا الله يستكبرون",
            "transliteration": "Innahum kanoo itha qeela lahum la ilaha illa Allahu yastakbiroona"
        },
        {
            "id": 36,
            "text": "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓاْ ءَالِهَتِنَا لِشَاعِرٖ مَّجۡنُونِۭ",
            "textNormalized": "ويقولون اينا لتاركوا ءالهتنا لشاعر مجنون",
            "transliteration": "Wayaqooloona ainna latarikoo alihatina lishaAAirin majnoonin"
        },
        {
            "id": 37,
            "text": "بَلۡ جَآءَ بِٱلۡحَقِّ وَصَدَّقَ ٱلۡمُرۡسَلِينَ",
            "textNormalized": "بل جاء بالحق وصدق المرسلين",
            "transliteration": "Bal jaa bialhaqqi wasaddaqa almursaleena"
        },
        {
            "id": 38,
            "text": "إِنَّكُمۡ لَذَآئِقُواْ ٱلۡعَذَابِ ٱلۡأَلِيمِ",
            "textNormalized": "انكم لذايقوا العذاب الاليم",
            "transliteration": "Innakum lathaiqoo alAAathabi alaleemi"
        },
        {
            "id": 39,
            "text": "وَمَا تُجۡزَوۡنَ إِلَّا مَا كُنتُمۡ تَعۡمَلُونَ",
            "textNormalized": "وما تجزون الا ما كنتم تعملون",
            "transliteration": "Wama tujzawna illa ma kuntum taAAmaloona"
        },
        {
            "id": 40,
            "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِينَ",
            "textNormalized": "الا عباد الله المخلصين",
            "transliteration": "Illa AAibada Allahi almukhlaseena"
        },
        {
            "id": 41,
            "text": "أُوْلَـٰٓئِكَ لَهُمۡ رِزۡقٞ مَّعۡلُومٞ",
            "textNormalized": "اوليك لهم رزق معلوم",
            "transliteration": "Olaika lahum rizqun maAAloomun"
        },
        {
            "id": 42,
            "text": "فَوَٰكِهُ وَهُم مُّكۡرَمُونَ",
            "textNormalized": "فوكه وهم مكرمون",
            "transliteration": "Fawakihu wahum mukramoona"
        },
        {
            "id": 43,
            "text": "فِي جَنَّـٰتِ ٱلنَّعِيمِ",
            "textNormalized": "في جنت النعيم",
            "transliteration": "Fee jannati alnnaAAeemi"
        },
        {
            "id": 44,
            "text": "عَلَىٰ سُرُرٖ مُّتَقَٰبِلِينَ",
            "textNormalized": "علي سرر متقبلين",
            "transliteration": "AAala sururin mutaqabileena"
        },
        {
            "id": 45,
            "text": "يُطَافُ عَلَيۡهِم بِكَأۡسٖ مِّن مَّعِينِۭ",
            "textNormalized": "يطاف عليهم بكاس من معين",
            "transliteration": "Yutafu AAalayhim bikasin min maAAeenin"
        },
        {
            "id": 46,
            "text": "بَيۡضَآءَ لَذَّةٖ لِّلشَّـٰرِبِينَ",
            "textNormalized": "بيضاء لذه للشربين",
            "transliteration": "Baydaa laththatin lilshsharibeena"
        },
        {
            "id": 47,
            "text": "لَا فِيهَا غَوۡلٞ وَلَا هُمۡ عَنۡهَا يُنزَفُونَ",
            "textNormalized": "لا فيها غول ولا هم عنها ينزفون",
            "transliteration": "La feeha ghawlun wala hum AAanha yunzafoona"
        },
        {
            "id": 48,
            "text": "وَعِندَهُمۡ قَٰصِرَٰتُ ٱلطَّرۡفِ عِينٞ",
            "textNormalized": "وعندهم قصرت الطرف عين",
            "transliteration": "WaAAindahum qasiratu alttarfi AAeenun"
        },
        {
            "id": 49,
            "text": "كَأَنَّهُنَّ بَيۡضٞ مَّكۡنُونٞ",
            "textNormalized": "كانهن بيض مكنون",
            "transliteration": "Kaannahunna baydun maknoonun"
        },
        {
            "id": 50,
            "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَسَآءَلُونَ",
            "textNormalized": "فاقبل بعضهم علي بعض يتساءلون",
            "transliteration": "Faaqbala baAAduhum AAala baAAdin yatasaaloona"
        },
        {
            "id": 51,
            "text": "قَالَ قَآئِلٞ مِّنۡهُمۡ إِنِّي كَانَ لِي قَرِينٞ",
            "textNormalized": "قال قايل منهم اني كان لي قرين",
            "transliteration": "Qala qailun minhum innee kana lee qareenun"
        },
        {
            "id": 52,
            "text": "يَقُولُ أَءِنَّكَ لَمِنَ ٱلۡمُصَدِّقِينَ",
            "textNormalized": "يقول اءنك لمن المصدقين",
            "transliteration": "Yaqoolu ainnaka lamina almusaddiqeena"
        },
        {
            "id": 53,
            "text": "أَءِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَدِينُونَ",
            "textNormalized": "اءذا متنا وكنا ترابا وعظما اءنا لمدينون",
            "transliteration": "Aitha mitna wakunna turaban waAAithaman ainna lamadeenoona"
        },
        {
            "id": 54,
            "text": "قَالَ هَلۡ أَنتُم مُّطَّلِعُونَ",
            "textNormalized": "قال هل انتم مطلعون",
            "transliteration": "Qala hal antum muttaliAAoona"
        },
        {
            "id": 55,
            "text": "فَٱطَّلَعَ فَرَءَاهُ فِي سَوَآءِ ٱلۡجَحِيمِ",
            "textNormalized": "فاطلع فرءاه في سواء الجحيم",
            "transliteration": "FaittalaAAa faraahu fee sawai aljaheemi"
        },
        {
            "id": 56,
            "text": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرۡدِينِ",
            "textNormalized": "قال تالله ان كدت لتردين",
            "transliteration": "Qala taAllahi in kidta laturdeeni"
        },
        {
            "id": 57,
            "text": "وَلَوۡلَا نِعۡمَةُ رَبِّي لَكُنتُ مِنَ ٱلۡمُحۡضَرِينَ",
            "textNormalized": "ولولا نعمه ربي لكنت من المحضرين",
            "transliteration": "Walawla niAAmatu rabbee lakuntu mina almuhdareena"
        },
        {
            "id": 58,
            "text": "أَفَمَا نَحۡنُ بِمَيِّتِينَ",
            "textNormalized": "افما نحن بميتين",
            "transliteration": "Afama nahnu bimayyiteena"
        },
        {
            "id": 59,
            "text": "إِلَّا مَوۡتَتَنَا ٱلۡأُولَىٰ وَمَا نَحۡنُ بِمُعَذَّبِينَ",
            "textNormalized": "الا موتتنا الاولي وما نحن بمعذبين",
            "transliteration": "Illa mawtatana aloola wama nahnu bimuAAaththabeena"
        },
        {
            "id": 60,
            "text": "إِنَّ هَٰذَا لَهُوَ ٱلۡفَوۡزُ ٱلۡعَظِيمُ",
            "textNormalized": "ان هذا لهو الفوز العظيم",
            "transliteration": "Inna hatha lahuwa alfawzu alAAatheemu"
        },
        {
            "id": 61,
