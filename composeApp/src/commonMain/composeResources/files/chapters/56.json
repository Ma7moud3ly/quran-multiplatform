{
    "id": 56,
    "name": "الواقعة",
    "transliteration": "Al-Waqi'ah",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ",
            "textNormalized": "اذا وقعت الواقعه",
            "transliteration": "Itha waqaAAati alwaqiAAatu"
        },
        {
            "id": 2,
            "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ",
            "textNormalized": "ليس لوقعتها كاذبه",
            "transliteration": "Laysa liwaqAAatiha kathibatun"
        },
        {
            "id": 3,
            "text": "خَافِضَةٞ رَّافِعَةٌ",
            "textNormalized": "خافضه رافعه",
            "transliteration": "Khafidatun rafiAAatun"
        },
        {
            "id": 4,
            "text": "إِذَا رُجَّتِ ٱلۡأَرۡضُ رَجّٗا",
            "textNormalized": "اذا رجت الارض رجا",
            "transliteration": "Itha rujjati alardu rajjan"
        },
        {
            "id": 5,
            "text": "وَبُسَّتِ ٱلۡجِبَالُ بَسّٗا",
            "textNormalized": "وبست الجبال بسا",
            "transliteration": "Wabussati aljibalu bassan"
        },
        {
            "id": 6,
            "text": "فَكَانَتۡ هَبَآءٗ مُّنۢبَثّٗا",
            "textNormalized": "فكانت هباء منبثا",
            "transliteration": "Fakanat habaan munbaththan"
        },
        {
            "id": 7,
            "text": "وَكُنتُمۡ أَزۡوَٰجٗا ثَلَٰثَةٗ",
            "textNormalized": "وكنتم ازوجا ثلثه",
            "transliteration": "Wakuntum azwajan thalathatan"
        },
        {
            "id": 8,
            "text": "فَأَصۡحَٰبُ ٱلۡمَيۡمَنَةِ مَآ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
            "textNormalized": "فاصحب الميمنه ما اصحب الميمنه",
            "transliteration": "Faashabu almaymanati ma ashabu almaymanati"
        },
        {
            "id": 9,
            "text": "وَأَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ مَآ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
            "textNormalized": "واصحب المشمه ما اصحب المشمه",
            "transliteration": "Waashabu almashamati ma ashabu almashamati"
        },
        {
            "id": 10,
            "text": "وَٱلسَّـٰبِقُونَ ٱلسَّـٰبِقُونَ",
            "textNormalized": "والسبقون السبقون",
            "transliteration": "Waalssabiqoona alssabiqoona"
        },
        {
            "id": 11,
            "text": "أُوْلَـٰٓئِكَ ٱلۡمُقَرَّبُونَ",
            "textNormalized": "اوليك المقربون",
            "transliteration": "Olaika almuqarraboona"
        },
        {
            "id": 12,
            "text": "فِي جَنَّـٰتِ ٱلنَّعِيمِ",
            "textNormalized": "في جنت النعيم",
            "transliteration": "Fee jannati alnnaAAeemi"
        },
        {
            "id": 13,
            "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
            "textNormalized": "ثله من الاولين",
            "transliteration": "Thullatun mina alawwaleena"
        },
        {
            "id": 14,
            "text": "وَقَلِيلٞ مِّنَ ٱلۡأٓخِرِينَ",
            "textNormalized": "وقليل من الاخرين",
            "transliteration": "Waqaleelun mina alakhireena"
        },
        {
            "id": 15,
            "text": "عَلَىٰ سُرُرٖ مَّوۡضُونَةٖ",
            "textNormalized": "علي سرر موضونه",
            "transliteration": "AAala sururin mawdoonatin"
        },
        {
            "id": 16,
            "text": "مُّتَّكِـِٔينَ عَلَيۡهَا مُتَقَٰبِلِينَ",
            "textNormalized": "متكين عليها متقبلين",
            "transliteration": "Muttakieena AAalayha mutaqabileena"
        },
        {
            "id": 17,
            "text": "يَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ",
            "textNormalized": "يطوف عليهم ولدن مخلدون",
            "transliteration": "Yatoofu AAalayhim wildanun mukhalladoona"
        },
        {
            "id": 18,
            "text": "بِأَكۡوَابٖ وَأَبَارِيقَ وَكَأۡسٖ مِّن مَّعِينٖ",
            "textNormalized": "باكواب واباريق وكاس من معين",
