{
    "id": 91,
    "name": "الشمس",
    "transliteration": "Ash-Shams",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا",
            "textNormalized": "والشمس وضحيها",
            "transliteration": "Waalshshamsi waduhaha"
        },
        {
            "id": 2,
            "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا",
            "textNormalized": "والقمر اذا تليها",
            "transliteration": "Waalqamari itha talaha"
        },
        {
            "id": 3,
            "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
            "textNormalized": "والنهار اذا جليها",
            "transliteration": "Waalnnahari itha jallaha"
        },
        {
            "id": 4,
            "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰهَا",
            "textNormalized": "واليل اذا يغشيها",
            "transliteration": "Waallayli itha yaghshaha"
        },
        {
            "id": 5,
            "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
            "textNormalized": "والسماء وما بنيها",
            "transliteration": "Waalssamai wama banaha"
        },
        {
            "id": 6,
            "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا",
            "textNormalized": "والارض وما طحيها",
            "transliteration": "Waalardi wama tahaha"
        },
        {
            "id": 7,
            "text": "وَنَفۡسٖ وَمَا سَوَّىٰهَا",
            "textNormalized": "ونفس وما سويها",
            "transliteration": "Wanafsin wama sawwaha"
        },
        {
            "id": 8,
            "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا",
            "textNormalized": "فالهمها فجورها وتقويها",
            "transliteration": "Faalhamaha fujooraha wataqwaha"
        },
        {
            "id": 9,
            "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا",
            "textNormalized": "قد افلح من زكيها",
            "transliteration": "Qad aflaha man zakkaha"
        },
        {
            "id": 10,
            "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا",
            "textNormalized": "وقد خاب من دسيها",
            "transliteration": "Waqad khaba man dassaha"
        },
        {
            "id": 11,
            "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَآ",
            "textNormalized": "كذبت ثمود بطغويها",
            "transliteration": "Kaththabat thamoodu bitaghwaha"
        },
        {
            "id": 12,
            "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا",
            "textNormalized": "اذ انبعث اشقيها",
            "transliteration": "Ithi inbaAAatha ashqaha"
        },
        {
            "id": 13,
            "text": "فَقَالَ لَهُمۡ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡيَٰهَا",
            "textNormalized": "فقال لهم رسول الله ناقه الله وسقيها",
            "transliteration": "Faqala lahum rasoolu Allahi naqata Allahi wasuqyaha"
        },
        {
            "id": 14,
            "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوَّىٰهَا",
            "textNormalized": "فكذبوه فعقروها فدمدم عليهم ربهم بذنبهم فسويها",
            "transliteration": "Fakaththaboohu faAAaqarooha fadamdama AAalayhim rabbuhum bithanbihim fasawwaha"
        },
        {
            "id": 15,
            "text": "وَلَا يَخَافُ عُقۡبَٰهَا",
            "textNormalized": "ولا يخاف عقبها",
            "transliteration": "Wala yakhafu AAuqbaha"
        }
    ]
}