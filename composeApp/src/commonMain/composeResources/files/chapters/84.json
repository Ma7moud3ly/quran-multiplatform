{
    "id": 84,
    "name": "الإنشقاق",
    "transliteration": "Al-Inshiqaq",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "إِذَا ٱلسَّمَآءُ ٱنشَقَّتۡ",
            "textNormalized": "اذا السماء انشقت",
            "transliteration": "Itha alssamao inshaqqat"
        },
        {
            "id": 2,
            "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ",
            "textNormalized": "واذنت لربها وحقت",
            "transliteration": "Waathinat lirabbiha wahuqqat"
        },
        {
            "id": 3,
            "text": "وَإِذَا ٱلۡأَرۡضُ مُدَّتۡ",
            "textNormalized": "واذا الارض مدت",
            "transliteration": "Waitha alardu muddat"
        },
        {
            "id": 4,
            "text": "وَأَلۡقَتۡ مَا فِيهَا وَتَخَلَّتۡ",
            "textNormalized": "والقت ما فيها وتخلت",
            "transliteration": "Waalqat ma feeha watakhallat"
        },
        {
            "id": 5,
            "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ",
            "textNormalized": "واذنت لربها وحقت",
            "transliteration": "Waathinat lirabbiha wahuqqat"
        },
        {
            "id": 6,
            "text": "يَـٰٓأَيُّهَا ٱلۡإِنسَٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدۡحٗا فَمُلَٰقِيهِ",
            "textNormalized": "يايها الانسن انك كادح الي ربك كدحا فملقيه",
            "transliteration": "Ya ayyuha alinsanu innaka kadihun ila rabbika kadhan famulaqeehi"
        },
        {
            "id": 7,
            "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ",
            "textNormalized": "فاما من اوتي كتبه بيمينه",
            "transliteration": "Faama man ootiya kitabahu biyameenihi"
        },
        {
            "id": 8,
            "text": "فَسَوۡفَ يُحَاسَبُ حِسَابٗا يَسِيرٗا",
            "textNormalized": "فسوف يحاسب حسابا يسيرا",
            "transliteration": "Fasawfa yuhasabu hisaban yaseeran"
        },
        {
            "id": 9,
            "text": "وَيَنقَلِبُ إِلَىٰٓ أَهۡلِهِۦ مَسۡرُورٗا",
            "textNormalized": "وينقلب الي اهله مسرورا",
            "transliteration": "Wayanqalibu ila ahlihi masrooran"
        },
        {
            "id": 10,
            "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ وَرَآءَ ظَهۡرِهِۦ",
            "textNormalized": "واما من اوتي كتبه وراء ظهره",
            "transliteration": "Waamma man ootiya kitabahu waraa thahrihi"
        },
        {
            "id": 11,
            "text": "فَسَوۡفَ يَدۡعُواْ ثُبُورٗا",
            "textNormalized": "فسوف يدعوا ثبورا",
            "transliteration": "Fasawfa yadAAoo thubooran"
        },
        {
            "id": 12,
            "text": "وَيَصۡلَىٰ سَعِيرًا",
            "textNormalized": "ويصلي سعيرا",
            "transliteration": "Wayasla saAAeeran"
        },
        {
            "id": 13,
            "text": "إِنَّهُۥ كَانَ فِيٓ أَهۡلِهِۦ مَسۡرُورًا",
            "textNormalized": "انه كان في اهله مسرورا",
            "transliteration": "Innahu kana fee ahlihi masrooran"
        },
        {
            "id": 14,
            "text": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ",
            "textNormalized": "انه ظن ان لن يحور",
            "transliteration": "Innahu thanna an lan yahoora"
        },
        {
            "id": 15,
            "text": "بَلَىٰٓۚ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرٗا",
            "textNormalized": "بلي ان ربه كان به بصيرا",
            "transliteration": "Bala inna rabbahu kana bihi baseeran"
        },
        {
            "id": 16,
            "text": "فَلَآ أُقۡسِمُ بِٱلشَّفَقِ",
            "textNormalized": "فلا اقسم بالشفق",
            "transliteration": "Fala oqsimu bialshshafaqi"
        },
        {
            "id": 17,
            "text": "وَٱلَّيۡلِ وَمَا وَسَقَ",
            "textNormalized": "واليل وما وسق",
            "transliteration": "Waallayli wama wasaqa"
        },
        {
            "id": 18,
            "text": "وَٱلۡقَمَرِ إِذَا ٱتَّسَقَ",
            "textNormalized": "والقمر اذا اتسق",
            "transliteration": "Waalqamari itha ittasaqa"
        },
        {
            "id": 19,
            "text": "لَتَرۡكَبُنَّ طَبَقًا عَن طَبَقٖ",
            "textNormalized": "لتركبن طبقا عن طبق",
            "transliteration": "Latarkabunna tabaqan AAan tabaqin"
        },
        {
            "id": 20,
            "text": "فَمَا لَهُمۡ لَا يُؤۡمِنُونَ",
            "textNormalized": "فما لهم لا يومنون",
            "transliteration": "Fama lahum la yuminoona"
        },
        {
            "id": 21,
            "text": "وَإِذَا قُرِئَ عَلَيۡهِمُ ٱلۡقُرۡءَانُ لَا يَسۡجُدُونَۤ۩",
            "textNormalized": "واذا قري عليهم القرءان لا يسجدون",
            "transliteration": "Waitha quria AAalayhimu alquranu la yasjudoona"
        },
        {
            "id": 22,
            "text": "بَلِ ٱلَّذِينَ كَفَرُواْ يُكَذِّبُونَ",
            "textNormalized": "بل الذين كفروا يكذبون",
            "transliteration": "Bali allatheena kafaroo yukaththiboona"
        },
        {
            "id": 23,
            "text": "وَٱللَّهُ أَعۡلَمُ بِمَا يُوعُونَ",
            "textNormalized": "والله اعلم بما يوعون",
            "transliteration": "WaAllahu aAAlamu bima yooAAoona"
        },
        {
            "id": 24,
            "text": "فَبَشِّرۡهُم بِعَذَابٍ أَلِيمٍ",
            "textNormalized": "فبشرهم بعذاب اليم",
            "transliteration": "Fabashshirhum biAAathabin aleemin"
        },
        {
            "id": 25,
            "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّـٰلِحَٰتِ لَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونِۭ",
            "textNormalized": "الا الذين ءامنوا وعملوا الصلحت لهم اجر غير ممنون",
            "transliteration": "Illa allatheena amanoo waAAamiloo alssalihati lahum ajrun ghayru mamnoonin"
        }
    ]
}