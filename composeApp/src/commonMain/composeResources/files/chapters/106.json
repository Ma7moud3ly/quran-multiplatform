{
    "id": 106,
    "name": "قريش",
    "transliteration": "Quraysh",
    "type": "meccan",
    "verses": [
        {
            "id": 1,
            "text": "لِإِيلَٰفِ قُرَيۡشٍ",
            "textNormalized": "لايلف قريش",
            "transliteration": "Lieelafi qurayshin"
        },
        {
            "id": 2,
            "text": "إِۦلَٰفِهِمۡ رِحۡلَةَ ٱلشِّتَآءِ وَٱلصَّيۡفِ",
            "textNormalized": "الفهم رحله الشتاء والصيف",
            "transliteration": "Eelafihim rihlata alshshitai waalssayfi"
        },
        {
            "id": 3,
            "text": "فَلۡيَعۡبُدُواْ رَبَّ هَٰذَا ٱلۡبَيۡتِ",
            "textNormalized": "فليعبدوا رب هذا البيت",
            "transliteration": "FalyaAAbudoo rabba hatha albayti"
        },
        {
            "id": 4,
            "text": "ٱلَّذِيٓ أَطۡعَمَهُم مِّن جُوعٖ وَءَامَنَهُم مِّنۡ خَوۡفِۭ",
            "textNormalized": "الذي اطعمهم من جوع وءامنهم من خوف",
            "transliteration": "Allathee atAAamahum min jooAAin waamanahum min khawfin"
        }
    ]
}